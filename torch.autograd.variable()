import numpy as np
import torch
import torch.nn
 
datas = np.array([[1, 2], [3, 4]])
labels = np.array([[2, 3], [4, 5]])
loss_fn = torch.nn.MSELoss()

input1 = torch.autograd.Variable(torch.from_numpy(datas).float(), requires_grad=True)
target1 = torch.autograd.Variable(torch.from_numpy(labels).float(), requires_grad=True)
print(input1)
print(target1)
#tensor([[1., 2.],
#        [3., 4.]], requires_grad=True)
#tensor([[2., 3.],
#        [4., 5.]], requires_grad=True)
 
loss = loss_fn(input1, target1)
print(loss)
#tensor(1., grad_fn=<MseLossBackward0>)
